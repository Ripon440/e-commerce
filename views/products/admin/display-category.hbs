<div class="row p-0">
    <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3 mt-2">
        {{> dashboard_list }}
    </div>
    <div class="col-xs-12 col-sm-12 col-md-9 col-lg-9">
        <div class=" text-center white p-2 mb-2 mt-2">
            <h2 class="red-candy-color">All Catagory</h2>
        </div>
        
        <div class="row">
            <div class="col">
                <ul class="list-group">
                    {{# each category}}
                        {{# each this }}
                            <li class="list-group-item d-flex justify-content-between align-items-center">
                                {{ this.categoryName }}
                                <span class="badge badge-primary badge-pill"><a href="/products/delete-category/{{ this._id }}" style="color:#fff; font-size:14px">Delete</a></span>
                            </li>
                        {{/each}}
                    {{/each}}
                </ul>
            </div>
        </div>
    </div>
</div>
    
    
